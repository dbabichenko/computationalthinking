   "outputs": [],
   "source": [
    "def get_tweets(handle):\n",
    "    tweets = []\n",
    "    # use the user_timeline function of the api object to get a list of status objects\n",
    "    new_tweets = api.user_timeline(screen_name = handle,count=200, tweet_mode=\"extended\", wait_on_rate_limit=True, compression=True)\n",
    "    # iterate through the list of status objects to extract only the text of those tweets\n",
    "    for t in new_tweets:\n",
    "        # use full_text instead of simply text because we're in extended mode, to get tweets of length beyond the old maximum length\n",
    "        tweets.append(t.full_text)\n",
    "    return tweets"
   ]
